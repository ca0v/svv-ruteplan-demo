<configuration>
  <appSettings>
    <add key="RouterBackendUrl" value="http://localhost:2062/RoutingService/RoutingService?"/>
    <!--<add key="RouterBackendUrl" value="http://multirit.triona.se/routingService_v1_0/routingService?"/>-->
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <handlers>
      <add name="router" verb="*" path="/routingService" type="Ruteplanwebb.Route"/>
      <add name="wmsCapabilities" verb="*" path="/wmsCapabilities" type="Ruteplanwebb.WmsProxy"/>
    </handlers>
  </system.webServer>
</configuration>